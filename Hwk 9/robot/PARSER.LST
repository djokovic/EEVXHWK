0003 C70600000000   R      569         MOV Errorflag, FALSE            ;Assume no errors
                           570             
0009                       571     ParseGrabTokens:        
0009 E84600                572             CALL    GetTokenTypeVal         ; Grab next token key and val
000C 8AF4                  573             MOV             DH, AH                      ; Save token type
000E 8AE8                  574             MOV             CH, AL                      ; Save token val
                           575             
0010                       576     ParseComputeTrans:                      ;figure out what transition to do
0010 B00D                  577             MOV             AL, NUM_TOKEN_TYPES     ;find row in the table
0012 F6260500       R      578             MUL             FSM_State           ;Get to current FSM state
